    CarRent.Models.Kullanici aktifYon = (CarRent.Models.Kullanici)Session["Yonetici"];
}
<!DOCTYPE html>
<html>
<head>
    <title>Car Rental An Auto mobile Category Flat Bootstrap Responsive Website Template | Home :: w3layouts</title>
    <!-- for-mobile-apps -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="" />
    <script type="application/x-javascript">
        addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
               function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- //for-mobile-apps -->
    @*<link href="/Content/web/css/bootstrap.css" rel="stylesheet" type="text/css" media="all" />*@

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">


    <link href="/Content/web/css/galleryeffect.css" rel="stylesheet" type="text/css" media="all" />
    <link rel="stylesheet" href="~/Content/web/css/jquery.flipster.css">
    <link rel='stylesheet' href='~/Content/web/css/dscountdown.css' type='text/css' media='all' />
    <link href="~/Content/web/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/Content/web/css/font-awesome.css" rel="stylesheet">


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
</head>
<body>

    <!-- banner -->
    <div class="container">
        <div class="header-nav">
            <nav class="navbar navbar-default">
                <div class="row">
                    <div class="col-md-6">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <h1><a href="/Home/Anasayfa"><span class="logo-c">Bi</span><i class="fa fa-car" aria-hidden="true"></i>Araba</a><p class="sub-cap">Herkese bi araba!</p></h1>
                        </div>
                    </div>
                    <!-- navbar-header -->
                    <div class="col-md-6">
                        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                            <ul class="nav navbar-nav navbar-right">
                                <li><a href="/Home/Anasayfa" class="hvr-underline-from-center active">Anasayfa</a></li>
                                @if (aktifKul != null && aktifYon == null)
                                {
                                    <li><a href="/Home/Araclarim" class="hvr-underline-from-center active">@aktifKul.ad @aktifKul.soyad</a></li>
                                    <li><a href="/Home/Cikis" class="hvr-underline-from-center active">Çıkış</a></li>
                                }
                                else if (aktifYon != null && aktifKul == null)
                                {
                                    <li><a href="/Panel/Index" class="hvr-underline-from-center active">@aktifYon.ad @aktifYon.soyad</a></li>
                                    <li><a href="/Home/Cikis" class="hvr-underline-from-center active">Çıkış</a></li>
                                }
                                else if (aktifYon != null && aktifKul != null)
                                {
                                    <li><a href="/Panel/Index" class="hvr-underline-from-center active">@aktifYon.ad @aktifYon.soyad</a></li>
                                    <li><a href="/Home/Cikis" class="hvr-underline-from-center active">Çıkış</a></li>
                                }
                                else
                                {
                                    <li><a href="/Login/Index" class="hvr-underline-from-center active">Giriş</a></li>
                                    <li><a class="hvr-underline-from-center active" data-toggle="modal" data-target="#KayitOl">KayitOl</a></li>
                                }

                            </ul>
                        </div>
                    </div>
                </div>
                <div class="clearfix"> </div>
            </nav>
        </div>
    </div>
    @RenderBody()
    <!-- footer -->
    <div class="newsletter">
        <div class="container">
            <div class="col-md-3 footer-grid">
                <h3>Hakkımızda</h3>
                <p>Rent a car sistemi</p>
                <div class="w3ls_newsletter_social">
                    <ul class="agileits_social_list">
